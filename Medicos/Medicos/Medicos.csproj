<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\consentimiento.pdf" />
    <None Remove="Assets\consolidado.pdf" />
    <None Remove="Assets\consolidado_a4.pdf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\consentimiento.pdf" />
    <EmbeddedResource Include="Assets\consolidado.pdf" />
    <EmbeddedResource Include="Assets\consolidado_a4.pdf" />
  </ItemGroup>

  <ItemGroup>
